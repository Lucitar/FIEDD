    <link rel="icon" href="./img/fav.png ">
    <title>Fiedd - Área de Chat - Redes</title>
  </head>
  <body>


    <%- menuprof %>

      <header class="header box effect">
        <h1 style="color: white;">Chat - Escolha</h1>
      </header>

  <section class="container_form">

    <form method="POST" action="/dashboard_professor/chat_entrar" style="color: #ffffff;">
        <h3 class="rowRadio" >Selecione o chat de uma turma:</h3>

        <div class="rowRadio rd">
        <input type="radio" name="escolha" value="administracao" id="administracao">
        <label for="administracao" style="color: #ffffff;">Administração</label>
        </div>

        <div class="rowRadio rd">
        <input type="radio" name="escolha" value="automacao" id="automacao">
        <label for="automacao" style="color: #ffffff;">Automação</label>
        </div>

        <div class="rowRadio rd">
        <input type="radio" name="escolha" value="edificacoes" id="edificacoes">
        <label for="edificacoes" style="color: #ffffff;">Edificações</label>
        </div>

        <div class="rowRadio rd">
        <input type="radio" name="escolha" value="massoterapia" id="massoterapia">
        <label for="massoterapia" style="color: #ffffff;">Massoterapia</label>
        </div>

        <div class="rowRadio rd">
        <input type="radio" name="escolha" value="redes" id="redes">
        <label for="redes" style="color: #ffffff;">Redes</label>
        </div>
        
        <div class="rowRadio rd">
          <input type="submit" value="Enviar" class="submit"> 
        </div>
      </form>

     </section>

     <!-- FIM DA SERVIÇOS SECTION -->
    <!-- JQUERY JS -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

